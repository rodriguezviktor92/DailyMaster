{
  "manifest_version":2,
  "name": "DailyMaster",
  "description": "DailyMaster",
  "version": "1.0",
  "icons":{
    "128": "schedule.png"
  },
  "browser_action":{
    "default_icon": "schedule.png"
  },
  "permissions":[
    "activeTab"
  ],
  "background":{
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "css": ["css/style.css","lib/main.css"],
        "js": ["lib/main.js", "events.js"]
    }
  ],
  "web_accessible_resources": [
    "images/calendar.png"
  ]
}